        <template id="fevexpo_footer_template" name="FEVEXPO Footer Template">
            <section>
                <div class="o_footer_fevexpo">
                    <div class="container">

                        <div class="row justify-content-between mt-5 pt-3">
                            <div class="col-12 col-sm text-left">
                                <h3 class="text-uppercase mb-2 myriadpro-semibold-condensed footer-encabezado">Queremos
                                    conocerte
                                </h3>
                                <p class="mt-4 myriadpro-regular texto text-white">
                                    Estamos a tu servicio, tu opinión nos importa.
                                    <br/>
                                    ¿Quieres conocernos mejor? ¿Quieres ser patrocinador o asociarte a nuestro evento?
                                </p>
                                <a href="#"
                                   class="text-uppercase form-control rounded-pill feria-contact-us mt-5 text-white text-center myriadpro-regular texto text-white">
                                    Contacta con nosotros
                                </a>
                            </div>
                            <div class="col-12 col-sm text-center mt-4 mt-sm-0">
                                <h3 class="text-uppercase mb-2 myriadpro-semibold-condensed footer-encabezado">
                                    Síguenos
                                </h3>
                                <div class="mt-4">
                                    <a href="#" class="s_share_facebook mr-2" target="_blank" title="Facebook">
                                        <img class="facebook-footer"
                                             src="/df_website_front/static/src/img/Facebook_footer_reposo.svg"
                                             width="52"
                                             alt="..."/>
                                    </a>
                                    <a href="#" class="s_share_twitter" target="_blank" title="Twitter">
                                        <img class="twitter-footer"
                                             src="/df_website_front/static/src/img/Twitter_footer_reposo.svg" width="52"
                                             alt="..."/>
                                    </a>
                                    <a href="#" class="s_share_linkedin ml-2" target="_blank" title="Youtube">
                                        <img class="youtube-footer"
                                             src="/df_website_front/static/src/img/Youtube_footer_reposo.svg" width="52"
                                             alt="..."/>
                                    </a>
                                </div>
                            </div>
                            <div class="col-12 col-sm text-center mt-4 mt-sm-0">
                                <h3 class="text-uppercase mb-2 myriadpro-semibold-condensed footer-encabezado">
                                    Nosotros
                                </h3>
                                <img class="img-fluid mt-4" src="/df_website_front/static/src/img/LOGO DESOFT.png"/>
                                <div class="mt-4 ml-5 mr-5 text-left myriadpro-regular texto text-white">
                                    <div class=""><span class="fa fa-phone fa-fw mr-2"/>(+53) 7 204 7397
                                    </div>
                                    <div class=""><span class="fa fa-mobile fa-fw mr-2"/>(+53) 5 805 1651
                                    </div>
                                    <div class=""><span class="fa fa-envelope fa-fw mr-2"/>eventovirtual@desoft.cu
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="footer-copyright-fevexpo text-center text-regular py-2">
                                <span>
                                    Copyright © 2021 | Eventos Virtuales
                                    <a href="https://www.desoft.cu" target="new" class="text-white">Desoft</a>
                                    | Todos los derechos reservados |
                                    <img src="/df_website_front/static/src/img/marcaCuba_negativo.png"
                                         class="imagen-cuba-footer"/>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </template>

        <template id="feria_navbar_template" name="Feria Navbar Template">
            <header>
                <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top menu-navbar navbar-plataforma-virtual">
                    <a class="navbar-brand" href="/">
                        <!--                    <img src="/df_website_front/static/src/img/plataformasVIRTUALES.png"-->
                        <img src="/df_website_front/static/src/img/felti-logo.png"
                             class="d-inline-block img-fluid" alt="" loading="lazy" style="height: 50px;"/>
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse"
                            data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"/>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav mx-auto">
                            <li class="nav-item menu-link">
                                <a class="nav-link text-uppercase myriadpro-regular" href="/event">Eventos</a>
                            </li>
                            <li class="nav-item menu-link dropdown servicios-link">
                                <a class="nav-link dropdown-toggle text-uppercase myriadpro-regular" href="#"
                                   id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true"
                                   aria-expanded="false">
                                    Servicios
                                </a>
                                <div class="dropdown-menu m-0 border-0 emergent-menu rounded-0"
                                     aria-labelledby="navbarDropdown">
                                    <a class="dropdown-item emergent-item text-uppercase text-white myriadpro-regular"
                                       href="#">Planes
                                    </a>
                                    <a class="dropdown-item emergent-item text-uppercase text-white myriadpro-regular"
                                       href="#">Ofertas Adicionales
                                    </a>
                                </div>
                            </li>
                            <li class="nav-item menu-link">
                                <a class="nav-link text-uppercase myriadpro-regular" href="/news">Noticias</a>
                            </li>
                            <li class="nav-item menu-link dropdown acercade-link">
                                <a class="nav-link dropdown-toggle text-uppercase myriadpro-regular" href="#"
                                   id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true"
                                   aria-expanded="false">
                                    Acerca de
                                </a>
                                <div class="dropdown-menu m-0 border-0 emergent-menu rounded-0"
                                     aria-labelledby="navbarDropdown">
                                    <a class="dropdown-item emergent-item text-uppercase text-white myriadpro-regular"
                                       href="#">Que es FEVexpo
                                    </a>
                                    <a class="dropdown-item emergent-item text-uppercase text-white myriadpro-regular"
                                       href="#">Preguntas Frecuentes
                                    </a>
                                    <a class="dropdown-item emergent-item text-uppercase text-white myriadpro-regular"
                                       href="#">Contacto
                                    </a>
                                </div>
                            </li>
                        </ul>
                        <ul class="navbar-nav">
                            <t t-call="portal.placeholder_user_sign_in">
                                <t t-set="_item_class" t-value="'nav-item'"/>
                                <t t-set="_link_class" t-value="'nav-link'"/>
                            </t>
                            <t t-call="portal.user_dropdown">
                                <t t-set="_user_name" t-value="true"/>
                                <t t-set="_item_class" t-value="'nav-item dropdown'"/>
                                <t t-set="_link_class" t-value="'nav-link'"/>
                                <t t-set="_dropdown_menu_class" t-value="'dropdown-menu-right'"/>
                            </t>
                            <li class="nav-item dropdown">
                                <t t-set="active_lang" t-value="list(filter(lambda lg : lg[0] == lang, languages))[0]"/>
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink"
                                   role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <span class="myriadpro-regular" t-if="not no_text"
                                          t-esc="active_lang[2].split('/').pop()"/>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                                    <t t-foreach="languages" t-as="lg">
                                        <a t-att-href="url_for(request.httprequest.path + '?' + keep_query(), lang_code=lg[0])"
                                           class="dropdown-item js_change_lang"
                                           t-att-data-url_code="lg[1]">
                                            <span class="myriadpro-regular" t-if="not no_text"
                                                  t-esc="lg[2].split('/').pop()"/>
                                        </a>
                                    </t>
                                </div>
                            </li>
                            <li class="nav-item active">
                                <a class="nav-link" href="#">
                                    <img class="search-navbar" src="/df_website_front/static/src/img/search.svg"
                                         height="24"
                                         width="24" alt=""/>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/my/home">
                                    <img class="user-navbar" src="/df_website_front/static/src/img/user-3.svg"
                                         height="24"
                                         width="24" alt=""/>
                                </a>
                            </li>
                            <!--                        <li class="nav-item">-->
                            <!--                            <a class="nav-link" href="/web/session/logout?redirect=/">-->
                            <!--                                <img class="locked-navbar" src="/df_website_front/static/src/img/locked.svg" height="24"-->
                            <!--                                     width="24" alt=""/>-->
                            <!--                            </a>-->
                            <!--                        </li>-->
                        </ul>
                    </div>
                </nav>
            </header>
        </template>

